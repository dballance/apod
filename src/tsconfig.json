{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../dist/",
    "module": "es2015",
    "sourceMap": true,
    "importHelpers": true,
    "downlevelIteration": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react"
  },
  "exclude": ["test.ts", "**/*.spec.ts"]
}
