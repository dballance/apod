{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest",
        "tslint-config-standard",
        "tslint-config-prettier"
    ], 
    "rulesDirectory": [
        "tslint-plugin-prettier"
    ], 
    "rules": {
        "prettier": [
            true,
            {
                "singleQuote": true,
                "trailingComma": "none",
                "semi": false
            }
        ],
        "interface-name": [
            true,
            "never-prefix"
        ]
    }
}